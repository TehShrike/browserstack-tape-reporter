{
  "name": "browserstack-tape-reporter",
  "version": "1.0.2",
  "description": "Run your tape tests on BrowserStack",
  "main": "index.js",
  "scripts": {
    "start": "browserify index.js example/index.js -o example/bundle.js && browserstack-runner",
    "local": "browserify index.js example/index.js -o example/bundle.js && node example/bundle.js",
    "test": "echo \"Error: no test specified :-(\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/browserstack-tape-reporter.git"
  },
  "keywords": [
    "tape",
    "browserstack",
    "test",
    "testing"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/browserstack-tape-reporter/issues"
  },
  "homepage": "https://github.com/TehShrike/browserstack-tape-reporter#readme",
  "devDependencies": {
    "browserify": "^13.0.1",
    "browserstack-runner": "^0.4.4",
    "tape": "^4.6.0"
  }
}
